	keywords = {Quantum Physics, Computer Science - Computational Complexity},
	pages = {080501},
	file = {Preprint PDF:/home/grierjones/Zotero/storage/NUKDPVX5/Bremner et al. - 2016 - Average-case complexity versus approximate simulat.pdf:application/pdf;Snapshot:/home/grierjones/Zotero/storage/2SHU8I58/1504.html:text/html},
}

