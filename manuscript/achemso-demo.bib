@article{van_den_berg_model-free_2022,
	title = {Model-free readout-error mitigation for quantum expectation values},
	volume = {105},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.105.032620},
	doi = {10.1103/PhysRevA.105.032620},
	abstract = {Measurements on current quantum processors are subject to hardware imperfections that lead to readout errors. These errors manifest themselves as a bias in quantum expectation values. Here, we consider a very simple method that forces the bias in the expectation value to appear as a multiplicative factor that can be measured directly and removed at the cost of an increase in the sampling complexity for the observable. This method was previously discussed by Karalekas et al. [Quantum Sci. Technol. 5, 024003 (2020)] for the product of single-qubit readout-error models. Here, we formally analyze the method and show that it can be applied to general noise models. In particular, we show the method does not need to assume any specific form of the noise model and requires only that the noise is “weak” to avoid excessive sampling overhead. We prove bounds relating the error in the expectation value to the sample complexity.},
	number = {3},
	urldate = {2025-01-09},
	journal = {Phys. Rev. A},
	author = {van den Berg, Ewout and Minev, Zlatko K. and Temme, Kristan},
	month = mar,
	year = {2022},
	note = {Publisher: American Physical Society},
	pages = {032620},
	file = {Full Text PDF:/home/grierjones/Zotero/storage/ZKKCW4KA/van den Berg et al. - 2022 - Model-free readout-error mitigation for quantum expectation values.pdf:application/pdf},
}

