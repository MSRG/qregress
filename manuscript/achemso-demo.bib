	note = {Publisher: The Royal Society of Chemistry},
	pages = {13754--13769},
	file = {Full Text PDF:/Users/grierjones/Zotero/storage/LZTMXL5J/De et al. - 2016 - Comparing molecules and solids across structural a.pdf:application/pdf},
}

