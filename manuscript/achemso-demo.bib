	note = {Publisher: Frontiers},
	keywords = {Noisy intermediate-scale quantum (NISQ), Parameterized Quantum Circuit (PQC), quantum machine learning (QML), Quantum Neural Network (QNN), Quantum-Classical Hybrid Algorithms},
